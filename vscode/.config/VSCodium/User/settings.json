{
    "workbench.startupEditor": "newUntitledFile",
    "git.confirmSync": false,
    "git.autofetch": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "extensions.ignoreRecommendations": true,
    "explorer.autoReveal": false,
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "'Fira Code', 'Font Awesome 5 Free Regular', 'Font Awesome 5 Brands Regular'",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "window.zoomLevel": 1,
    "files.associations": {
        "*.toml": "ini"
    },
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "python.jediEnabled": false,
    "projectManager.any.baseFolders": [
        "~/Documents"
    ],
    "projectManager.git.baseFolders": [
        "~/Documents"
    ],
    "projectManager.any.maxDepthRecursion": 1,
    "cmake.configureOnOpen": true,
    "workbench.tree.indent": 20,
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "konsole",
    "C_Cpp.updateChannel": "Insiders",
    "editor.codeLens": false,
    "[csharp]": {
        "editor.defaultFormatter": "Leopotam.csharpfixformat"
    },
    "files.exclude": {
        "**/*.meta": true
    },
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "telemetry.enableTelemetry": false,
    "git.detectSubmodules": false
}
